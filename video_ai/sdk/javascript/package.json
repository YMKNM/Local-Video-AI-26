{
  "name": "@video-ai/sdk",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript SDK for Video AI Platform",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "video-ai",
    "ai",
    "video-generation",
    "diffusion",
    "machine-learning",
    "nvidia",
    "cuda",
    "tensorrt"
  ],
  "author": "Video AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/video-ai/sdk-js"
  },
  "bugs": {
    "url": "https://github.com/video-ai/sdk-js/issues"
  },
  "homepage": "https://github.com/video-ai/sdk-js#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.50.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
